{
    "name": "express-ts-template",
    "version": "1.2.0",
    "description": "Simple starter template for ExpressJs and TypeScript based on expressjs/generator and TypeScript-Node-Starter",
    "main": "app.js",
    "scripts": {
        "start": "npm run serve",
        "build": "npm run build-ts",
        "serve": "node dist/app.js",
        "test": "jest --coverage=false --verbose",
        "watch": "nodemon src/app.ts",
        "build-ts": "tsc",
        "watch-ts": "tsc -w",
        "typecheck": "tsc --noEmit"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@types/knex": "^0.16.1",
        "dotenv": "^10.0.0",
        "express": "^4.16.2",
        "express-async-errors": "^3.1.1",
        "http-errors": "^1.7.1",
        "knex": "^0.95.14",
        "morgan": "^1.9.1",
        "mysql": "^2.18.1",
        "parse-database-url": "^0.3.0",
        "ts-node": "^10.4.0",
        "ts-transformer-keys": "^0.4.3"
    },
    "devDependencies": {
        "@types/express": "4.17.13",
        "@types/http-errors": "1.8.1",
        "@types/jest": "26.0.24",
        "@types/morgan": "1.9.3",
        "@types/node": "14.18.0",
        "@types/supertest": "2.0.11",
        "@typescript-eslint/eslint-plugin": "4.33.0",
        "@typescript-eslint/parser": "4.33.0",
        "concurrently": "6.4.0",
        "eslint": "7.32.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-import": "2.25.3",
        "jest": "26.6.3",
        "nodemon": "2.0.15",
        "prettier": "2.5.1",
        "supertest": "6.1.6",
        "ts-jest": "26.5.6",
        "typescript": "4.5.2"
    }
}
